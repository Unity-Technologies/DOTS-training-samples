<Project>

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <OutputPath>$(BaseTempPath)/bin/$(Configuration)/$(MSBuildProjectName)/</OutputPath>
    <IntermediateOutputPath>$(BaseTempPath)/obj/$(Configuration)/$(MSBuildProjectName)/</IntermediateOutputPath>
    <LangVersion>7.1</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="**\*.meta" />
    <None Remove="**\*.asmdef" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2019.1.1" />
  </ItemGroup>

</Project>
